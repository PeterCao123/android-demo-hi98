<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shiji.png.pat.app">

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="com.usdk.printer.perm.NORMAL_FUNC"/>

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.SettingsActivity"
            android:label="@string/settings" />
        <activity android:name=".ui.AboutActivity" />
        <activity
            android:name=".ui.EnterTableNoActivity"
            android:launchMode="singleTask" />
        <activity android:name=".ui.CheckAmountActivity" />
        <activity android:name=".ui.CardTypeActivity" />
        <activity android:name=".ui.AddTipActivity" />
        <activity android:name=".ui.PrintPreviewActivity"></activity>
    </application>

</manifest>